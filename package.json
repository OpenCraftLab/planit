{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "shopping-list",
  "version": "0.1.0-pre-alpha",
  "description": "Shopping list",
  "main": "src/main.ts",
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "start": "node --import tsx/esm src/main.ts",
    "dev": "tsx watch --env-file .env src/main.ts",
    "quality": "biome check --apply src",
    "migration:generate": "drizzle-kit generate:pg",
    "migration:push": "drizzle-kit push:pg",
    "migration:up": "drizzle-kit up:pg",
    "migration:check": "drizzle-kit check:pg",
    "migration:drop": "drizzle-kit drop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OpenCraftLab/shopping-list.git"
  },
  "keywords": [
    "shopping",
    "list"
  ],
  "author": "OpenCraftLab",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/OpenCraftLab/shopping-list/issues"
  },
  "homepage": "https://github.com/OpenCraftLab/shopping-list#readme",
  "packageManager": "pnpm@8.15.5",
  "dependencies": {
    "@aws-sdk/client-ses": "3.552.0",
    "@fastify/cors": "9.0.1",
    "@fastify/helmet": "11.1.1",
    "@fastify/jwt": "8.0.0",
    "@fastify/middie": "8.3.0",
    "@fastify/rate-limit": "9.1.0",
    "@fastify/swagger": "8.14.0",
    "@fastify/swagger-ui": "3.0.0",
    "@fastify/type-provider-typebox": "4.0.0",
    "@sinclair/typebox": "0.32.20",
    "drizzle-orm": "0.30.4",
    "fastify": "4.26.2",
    "pg": "8.11.3",
    "undici": "6.12.0",
    "zod": "3.22.4"
  },
  "devDependencies": {
    "@biomejs/biome": "1.6.2",
    "@types/jest": "29.5.12",
    "@types/node": "20.11.30",
    "@types/pg": "8.11.4",
    "drizzle-kit": "0.20.14",
    "jest": "29.7.0",
    "pino-pretty": "11.0.0",
    "ts-jest": "29.1.2",
    "tsx": "4.7.1",
    "typescript": "5.4.3"
  }
}
